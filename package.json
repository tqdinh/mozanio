{
  "name": "mozanio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-run-debug-dev": "react-native run-android --variant=devDebug --port 8081 --appId 'com.mozanio.dev'",
    "android-run-debug-staging": "react-native run-android --variant=stagingDebug --appId 'com.mozanio.staging'  --port 8084",
    "android-run-release-staging": "react-native run-android --variant=stagingRelease --appId 'com.mozanio.staging'",
    "android-run-release-product": "react-native run-android --variant=prodRelease --appId 'com.mozanio'",
    "ios": "react-native run-ios",
    "ios-run-debug-dev": "react-native run-ios --scheme KCampusDev --configuration DebugDev",
    "ios-run-debug-staging": "react-native run-ios --scheme MozanioStaging --configuration DebugStaging",
    "ios-run-release-staging": "react-native run-ios --scheme Mozanio --configuration ReleaseStaging",
    "ios-run-release-prod": "react-native run-ios --scheme KCampusProd --configuration ReleaseProd",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "prepare": "husky install"
  },
  "lint-staged": {
    "pre-commit": "lint-staged"
  },
